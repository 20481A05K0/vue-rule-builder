{
  "presets": [
    [
      "vue-app",
      {
        "modules": false
      }
    ],
    "vue"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".vue",
          ".json"
        ]
      }
    ],
    "external-helpers"
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node"
      ]
    }
  }
}
